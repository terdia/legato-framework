{
    "name": "legato/framework",
    "description": "The Legato Framework.",
    "keywords": ["framework", "legato"],
    "license": "MIT",
    "authors": [
        {
            "name": "Terry Osayawe Ogbemudia",
            "email":"terry@devscreencast.com",
            "homepage": "https://devscreencast.com"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Legato\\Framework\\":""
        },
        "files":["src/functions/helpers.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Framework\\Tests\\": "Tests/"
        }
    },
    "require": {
        "php": ">=7.0.0",
        "symfony/http-foundation": "^3.4",
        "illuminate/container": "^5.5",
        "vlucas/phpdotenv": "^2.4",
        "twig/twig": "^2.4",
        "guzzlehttp/guzzle": "^6.3",
        "filp/whoops": "^2.1",
        "philo/laravel-blade": "^3.1",
        "symfony/var-dumper": "^3.4",
        "symfony/thanks": "^1.0",
        "symfony/console": "^4.0",
        "symfony/filesystem": "^4.0",
        "illuminate/database": "^5.6",
        "swiftmailer/swiftmailer": "^6.0",
        "altorouter/altorouter": "^1.2"
    },
    "config":{
        "optimize-autoloader":true
    }
}
